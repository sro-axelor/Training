<?xml version="1.0" encoding="UTF-8"?>

<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

 <module name="companydetails" package="com.axelor.companydetails.db" />

<!-- entity definitions here -->
  <entity name="CompanyDepartment">
  
   <many-to-one name="companydetails" ref="CompanyDetails" required="true" />
  
  	<string name="departmentName" required="true" max="255"/>
    <integer name="departmentId" min="1" max="100"/>
  	<string name="departmentLocation"/>
  </entity>
</domain-models>