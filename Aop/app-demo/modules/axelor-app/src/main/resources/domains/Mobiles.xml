<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

<module name="app" package="com.axelor.app.db" />


<!-- entity -->
<entity name="Mobiles" cacheable="true">
	<string name="name" required="true"/>
	<string name="brand" required="true"/>
	<string name="model"/>
	<string name="color"/>
	<decimal name="price" required="true"/>
	
	
<!-- relations -->
<!-- 	<many-to-one name="orderlist" ref="Order" />  -->

	<many-to-many name="orderlist" ref="Order"/>
</entity>


</domain-models>