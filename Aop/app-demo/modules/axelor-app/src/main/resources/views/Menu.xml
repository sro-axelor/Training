<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

<!-- My Order menu -->
	<menuitem name="MyOrder-menu" title="My Order"
		icon="fas fa-shopping-cart" icon-background="pink"></menuitem>

	<menuitem name="menu-myorder-friends" parent="MyOrder-menu"
		title="Create Order" icon="fa-duotone fa-cart-plus"
		icon-background="brown" action="order.all" tag="click here"
		tag-style="info" />

	<action-view name="order.all" title="All Orders"
		model="com.axelor.app.db.Order">
		<view type="grid" name="order-grid" />
		<view type="form" name="order-form" />
		<view type="kanban" name="order-kaban" />
	</action-view>
	<!-- <menuitem name="menu-kanban" parent="MyOrder-menu" title="anbans" icon="fa-duotone 
		fa-cart-plus" action="kanban.all"/> -->

<!-- shipment -->
	<menuitem name="menu-shipment-friends" parent="MyOrder-menu"
		title="Shipment" action="shipment.all" tag="click here"
		tag-style="important" />

	<action-view name="shipment.all" title="All Shipments"
		model="com.axelor.app.db.Shipment">
		<view type="grid" name="shipment-grid" />
		<view type="form" name="shipment-form" />
		<view type="calendar" name="calendar-view" />
		<view type="gantt" name="shipment-gantt-view" />
	</action-view>

<!-- extensionview actions -->
	<!-- <action-view name="extenaion-panel-action" title="shipment" model="com.axelor.app.db.Shipment"> -->
	<!-- <view type="grid" name="transportlogistics-grid"/> -->
	<!-- <view type="form" name="transportlogistics-form"/> -->
