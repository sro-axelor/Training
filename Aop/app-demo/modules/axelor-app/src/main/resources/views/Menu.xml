<!-- master-details view -->
	<action-view name="two.login.view"
		model="com.axelor.app.db.LoginDetails" title="MasterView">
		<view name="logindetails-grid" type="grid" />
		<view-param name="details-view" value="true" />
	</action-view>



<!-- login menu -->
	<menuitem name="LoginDetails-menu" title="Log in"
		icon="fa-solid fa-user" icon-background="green" />

	<menuitem name="menu-logindetails-friends"
		parent="LoginDetails-menu" title="Fill Login Details"
		icon="fas fa-user-plus" icon-background="blue"
		action="logindetails.all" tag="click here" tag-style="important" />

	<action-view name="logindetails.all"
		title="All Login Details" model="com.axelor.app.db.LoginDetails">
		<view type="grid" name="logindetails-grid" />
		<view type="form" name="logindetails-form" />
		<view type="cards" name="login-cards" />
	</action-view>



</object-views>