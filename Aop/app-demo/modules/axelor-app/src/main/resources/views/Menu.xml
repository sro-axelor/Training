<!-- TransportLogistics -->
	<menuitem name="menu-transportlogistics-friends"
		parent="MyOrder-menu" title="TransportLogistics"
		action="transportlogistics.all" tag="click here" tag-style="inverse" />

	<action-view name="transportlogistics.all"
		title="All TransportLogistics"
		model="com.axelor.app.db.TransportLogistics">
		<view type="grid" name="transportlogistics-grid" />
		<view type="form" name="transportlogistics-form" />
	</action-view>


<!-- Mobiles parent-menu -->
	<menuitem name="AllMobiles-menu" title="Mobiles"
		icon="fas fa-mobile" icon-background="violet" />

	<menuitem name="Allmobiles-parent-menu" title="Select Mobile"
		parent="AllMobiles-menu" icon="fas fa-mobile" icon-background="orange"
		action="mobiles.all" tag="click here" tag-style="success" />

	<action-view name="mobiles.all" title="All Mobiles"
		model="com.axelor.app.db.Mobiles">
		<view type="grid" name="mobiles-grid" />
		<view type="form" name="mobiles-form" />
	</action-view>

<!-- SmartPhones child-menu -->
	<menuitem name="Allsmartphones-child-menu"
		title="Select SmartPhones" parent="AllMobiles-menu"
		icon="fas fa-mobile" icon-background="blue" action="smartphones.all"
		tag="click here" tag-style="default" />

	<action-view name="smartphones.all" title="All SmartPhones"
		model="com.axelor.app.db.SmartPhones">
		<view type="grid" name="smartphones-grid" />
		<view type="form" name="smartphones-form" />
	</action-view>

<!-- AndroidPhones grandchild-menu -->
	<menuitem name="Allandroidphones-grandchild-menu"
		title="Select AndroidPhones" parent="AllMobiles-menu"
		icon="fas fa-mobile" icon-background="pink" action="androidphones.all"
		tag="click here" tag-style="inverse" />

	<action-view name="androidphones.all"
		title="All AndroidPhones" model="com.axelor.app.db.AndroidPhones">
		<view type="grid" name="androidphones-grid" />
		<view type="form" name="androidphones-form" />
	</action-view>




<!-- tree view menu -->
<!-- <menuitem name="TreeView-menu" title="TreeView" parent="AllMobiles-menu" 
		action="treeview.all"/> -->

	<action-view name="treeview.all" title="All TreeView">
		<view type="tree" name="All-Mobiles-Tree" />
	</action-view>


<!-- kanban required actions -->
	<action-view name="onnew.action" title="onnew kanbanAction">
		<view type="form" name="mobiles-form" />
	</action-view>


<!-- custom view for dash-board view -->
	<action-view name="dashborad.action" title="Custom View">
		<view type="dashboard" name="order-dashboard" />
	</action-view>


<!-- chart-view action -->
	<action-view name="chartview.action" title="Chart View">
		<view type="dashboard" name="order-dashboard" />
	</action-view>


