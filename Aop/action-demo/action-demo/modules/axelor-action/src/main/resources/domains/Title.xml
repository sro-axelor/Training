<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models 
  https://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="action" package="com.axelor.action.db"/>

  <entity name="Title" cacheable="true">
      <string name="code" required="true" unique="true" min="2"/>
      <string name="name" required="true" unique="true" min="2" translatable="true" />
  </entity>

</domain-models>