<?xml version="1.0" encoding="UTF-8"?>

<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

	<module name="action" package="com.axelor.action.db" />

	<entity name="Item">
		<integer name="itemCode" />
		<string name="itemName" />
		<enum name="whichItem" ref="Whichitems" />
			
		<many-to-one name="orderes" ref="Order" />
	</entity>
	
	<enum name="Whichitems" >
	    <item name="MOBILES"/>
	    <item name="LAPTOPS"/>
	    <item name="TOYS"/>
	    <item name="CLOTHES"/>
	    <item name="FOOD"/>
	</enum>
	
</domain-models>